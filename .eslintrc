{
  "extends" : ["@visionappscz/eslint-config-visionapps"],
  "env": {
    "browser": true
  },
  "overrides": [
    {
      "files": [
        "**/index.js",
        "src/constants/*",
        "src/resources/*/*",
        "**/resources/*/actions/*",
        "src/services/**/*",
        "src/validators/**/*"
      ],
      "rules": {
        "import/prefer-default-export": "off"
      }
    }
  ]
}
